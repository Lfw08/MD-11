<PropertyList>

	<path>res/MCDU3.ac</path>
	
	<animation>
		<type>pick</type>
		<object-name>mcdu3.screen</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>nasal</command>
				<script>canvas_mcdu.showMcdu3();</script>
			</binding>
		</action>
	</animation>
	
	<effect>
		<inherits-from>Aircraft/MD-11/Models/Effects/displays/lcd</inherits-from>
		<parameters>
			<inner-angle>90</inner-angle> <!-- 0-90, use 90 for LED or plasma display-->
			<outer-angle>90</outer-angle> <!-- 0-90, but greater than inner -->
			<black-angle>90</black-angle> <!-- 0-90, but greater than outer -->
			<dirt-factor>0</dirt-factor>
			<contrast><use>instrumentation/du/crt-contrast</use></contrast>
		</parameters>
		<object-name>mcdu3.screen</object-name>
	</effect>
	
	<animation>
		<type>knob</type>
		<object-name>knob</object-name>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/du/mcdu3-dimmer</property>
				<factor>1</factor>
				<min>0</min>
				<max>1</max>
				<wrap>false</wrap>
			</binding>
		</action>
	</animation>
	
	<!-- Soft Keys -->
